<h3>Events</h3>
<table class="event_headline">
  <tr>
    <th>Title</th>
    <th>Date</th>
    <th>Location</th>
    <th></th>
  </tr>

  <% @events.each do |event| %>
    <tr>
      <td><%= event.title %></td>
      <td><%= event.date %></td>
      <td><%= event.location.name if event.location%></td>
      <td><%= link_to "Show", event_path(event) %></td>
    </tr>
  <% end %>
</table>

<h3>Users</h3>
<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Type</th>
    <th></th>
  </tr>
<% @users.each do |user| %>
  <tr>
    <td><%= user.first_name %></td>
    <td><%= user.email %></td>
    <td><%= user.user_type %></td>
    <td><%= link_to "Show", user_path(user) %></td>
  </tr>
<% end %>
</table>
    
    
<%= render :partial => "/partials/chat" %>